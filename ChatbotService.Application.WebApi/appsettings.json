{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KeyVaultClientDNS" : "https://kvchatbotservice.vault.azure.net/",
  "Settings": {
    "RabbitMqSettings": {
      "RabbitMqConnection": {
        "HostName": "shrimp.rmq.cloudamqp.com",
        "Port": "5672"
      },
      "ExchangeTelegram" : {
        "Type": "direct",
        "Queues": [
          {
            "Name": "service-telegram",
            "RoutingKeys": [ "service-to-telegram" ]
          }
        ]
      },
      "ExchangeService" : {
        "Type": "direct",
        "Queues": [
          {
            "Name": "telegram-service",
            "RoutingKeys": [ "telegram-to-service" ]
          }
        ]
      }
    },
    "MongoDbSettings": {
      "Url": "mongodb://ChatbotService:dev123@localhost:27017/?authMechanism=SCRAM-SHA-256&authSource=ChatbotService",
      "Database": "ChatbotService"
    }
  }
}
